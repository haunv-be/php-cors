{
    "name": "enlightener/php-cors",
    "description": "Enlightener PHP CORS (Cross-Origin Resource Sharing) is a small library support to prevent attacks from a cross-origin request",
    "keywords": ["cors", "php", "laravel", "symfony"],
    "homepage": "https://github.com/haunv-be/php-cors",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Van Hau",
            "email": "haunv.be@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Enlightener\\Cors\\": "src/Cors/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enlightener\\Test\\Cors\\": "Test/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "laravel/framework": "^6|^7|^8|^9|^10"
    },
    "minimum-stability": "dev"
}
